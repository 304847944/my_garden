<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TopBar">
        <attr name="leftBackground" format="reference"/>
        <attr name="rightBackground" format="reference"/>
        <attr name="customTitleText" format="string"/>
        <attr name="customTitleTextSize" format="dimension"/>
        <attr name="customTitleTextColor" format="color|reference"/>
    </declare-styleable>
    <declare-styleable name="PullToRefreshFrameLayout">
        <attr name="has_header" format="boolean" />
        <attr name="has_footer" format="boolean" />
        <attr name="has_shadow" format="boolean" />
        <attr name="has_divider" format="boolean" />
    </declare-styleable>
</resources>